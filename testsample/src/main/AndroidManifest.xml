<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.android.framework">

    <uses-permission android:name="android.permission.INTERNET" />

    <application android:allowBackup="true"
                 android:label="@string/app_name"
                 android:icon="@mipmap/app_icon"
                 android:theme="@style/AppTheme">

        <activity android:name="com.android.sample.HomeTestActivity"
            android:label="@string/activity_home_page"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name="com.android.sample.test_activity.ActivityTestHomePage"
            android:label="@string/activity_test_activity_homepage"
            android:screenOrientation="portrait">
        </activity>

        <activity android:name="com.android.sample.test_activity.ActivityA"
            android:label="@string/activity_test_activity_A"
            android:screenOrientation="portrait">
        </activity>
        
        <activity android:name="com.android.sample.test_activity.ActivityB"
            android:label="@string/activity_test_activity_B"
            android:screenOrientation="portrait">
        </activity>

    </application>

</manifest>
